<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <description>
    tag library 基本測試、實體字元測試
  </description>
  
  <taglib>
    <taglib-uri>
       Taglib1
    </taglib-uri>
    <taglib-location>
       /WEB-INF/tlds/MyTaglib1.tld
    </taglib-location> 
  </taglib>
  
  <taglib>
    <taglib-uri>
      /Taglib2
    </taglib-uri>
    <taglib-location>
      /WEB-INF/tlds/MyTaglib2.tld
    </taglib-location>
  </taglib>

</web-app>
